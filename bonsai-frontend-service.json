{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "dev-bonsai",
        "labels": {
            "role": "dev"
        }
    },
    "spec": {
        "type": "LoadBalancer",
        "ports": [
            {
                "port": 80,
                "targetPort": 8000,
                "protocol": "TCP"
            }
        ],
        "selector": {
            "name": "frontend",
            "role": "dev"
        }
    }
}
