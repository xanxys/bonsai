{
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
        "name": "dev-bonsai-frontend",
        "labels": {
            "name": "frontend",
            "role": "dev"
        }
    },
    "spec": {
        "containers": [
            {
                "name": "frontend",
                "image": "gcr.io/bonsai-genesis/bonsai_frontend",
                "ports": [
                    {
                        "containerPort": 8000
                    }
                ]
            }
        ]
    }
}
